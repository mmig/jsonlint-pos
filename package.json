{
  "author": "russa",
  "name": "jsonlint-ext",
  "description": "Validate JSON",
  "keywords": [
    "json",
    "validation",
    "lint",
    "jsonlint"
  ],
  "version": "1.6.2",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/russaa/jsonlint-ext.git"
  },
  "bugs": {
    "url": "https://github.com/russaa/jsonlint-ext/issues"
  },
  "main": "lib/jsonlint-ext.js",
  "bin": {
    "jsonlint": "lib/cli.js"
  },
  "browser": "web/jsonlint-ext.js",
  "engines": {
    "node": ">= 0.6"
  },
  "dependencies": {
    "JSV": ">= 4.0.x",
    "chalk": "^4.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "fs-extra": "^9.0.1",
    "jison": "^0.4.18",
    "test": "*",
    "uglify-js": "^3.10.0"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:jison": "node scripts/build-jison.js src/jsonlint.y src/jsonlint.l",
    "build:lib": "node scripts/bundle.js jsonlint.js lib/jsonlint-ext.js web/jsonlint-ext.js",
    "build:rm": "node -e require('fs').unlinkSync('jsonlint.js')",
    "test": "node test/all-tests.js"
  },
  "optionalDependencies": {}
}
